ex < $items.length - itemsToShow) {\n      currentIndex++;\n      updateSliderPosition();\n    }\n  });\n  $btnLeft.on('click', function () {\n    if (currentIndex > 0) {\n      currentIndex--;\n      updateSliderPosition();\n    }\n  });\n  var startX,\n    isDragging = false;\n  $container.on('mousedown', function (e) {\n    startX = e.pageX;\n    isDragging = true;\n    e.preventDefault(); // 기본 드래그 앤 드롭 기능 비활성화\n  });\n  $(document).on('mouseup', function (e) {\n    if (isDragging) {\n      isDragging = false;\n      updateSliderPosition(); // 마우스 버튼을 놓았을 때 슬라이드와 dot 상태 업데이트\n      e.preventDefault(); // 기본 드래그 앤 드롭 기능 비활성화\n    }\n  });\n  $container.on('mousemove', function (e) {\n    if (isDragging) {\n      e.preventDefault(); // 기본 드래그 앤 드롭 기능 비활성화\n      var diff = startX - e.pageX;\n      if (diff > 50 && currentIndex < $items.length - itemsToShow) {\n        currentIndex++;\n        updateSliderPosition();\n        isDragging = false;\n      } else if (diff < -50 && currentIndex > 0) {\n        currentIndex--;\n        updateSliderPosition();\n        isDragging = false;\n      }\n    }\n  });\n\n  // 초기 dot 생성 및 버튼 상태 업데이트\n  createDots();\n  updateButtonStates();\n});</script>\n</body>\n\n</html>"},"sourceMaps":null,"error":null,"hash":"677c8034a3397a4bc221c87617b77547","cacheData":{"env":{}}}